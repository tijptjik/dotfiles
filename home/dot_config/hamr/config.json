{
  "apps": {
    "terminal": "kitty",
    "terminalArgs" : "--class=floating.terminal",
    "shell": "fish"
  },
  "imageBrowser": {
    "useSystemFileDialog": false
  },
  "search": {
    "actionKeys": [
      "u",
      "i",
      "o",
      "p"
    ],
    "engineBaseUrl": "https://www.google.com/search?q=",
    "excludedSites": [
      "quora.com",
      "facebook.com"
    ],
    "nonAppResultDelay": 30,
    "prefix": {
      "action": "/",
      "app": ">",
      "clipboard": ";",
      "dict": "?",
      "emojis": ":",
      "file": "~",
      "math": "=",
      "shellCommand": "$",
      "shellHistory": "!"
    },
    "shellHistory": {
      "customHistoryPath": "",
      "enable": true,
      "maxEntries": 500,
      "shell": "auto"
    }
  },
  "behavior": {
    "clickOutsideAction": "close"
  },
  "appearance": {
    "fontScale": 1.3,
    "launcherYRatio": 0.2
  },
  "paths": {
    "wallpaperDir": "~/.local/share/backgrounds/21:9",
    "colorsJson": "~/.config/hamr/colors.json"
  }
}
